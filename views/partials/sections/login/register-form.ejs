<div class="col-xs-hidden col-sm-9">
    <div class="login-form-section">
        <a href="/login"><img src="public/images/login/logo.png" alt="" class="login-logo"></a>
        <div class="login-form register-form">
                <h2 class="form-header"><strong>Client</strong>Office</h2>
                <form>
                    <div class="form-group">
                        <label for="first_name">First name</label>
                        <input type="text" class="form-control" name="first_name" id="first_name" required>
                    </div>
                    <div class="form-group">
                        <label for="last_name">Last name</label>
                        <input type="text" class="form-control" name="last_name" id="last_name" required>
                    </div>
                    <div class="form-group">
                        <label for="country">Country</label>
                        <select name="country" class="form-control" id="country" required>
                            <option value="RO">Romania</option>
                            <option value="UK">United Kingdom</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label for="phone">Phone number</label>
                        <input type="tel" class="form-control" name="phone" id="phone" required>
                    </div>
                    <div class="form-group">
                        <label for="email">Email</label>
                        <input type="email" class="form-control" name="email" id="email" required>
                    </div>
                    <div class="checkbox">
                        <label><input type="checkbox" required>
                            I have read and I accept the <a href="">Terms and Conditions</a>, <a href="">Privacy Policy</a> and
                            <a href="">Disclaimer</a>.
                            I also agree to be contacted by one of your customer service representatives.
                        </label>
                    </div>
                    <br>
                    <div class="form-button-placeholder text-center">
                        <a href="/register-final-step" class="btn btn-default btn-form-black">Next <i class="fa fa-arrow-right" aria-hidden="true"></i></a>
                    </div>
                </form>
            </div>
    </div>
</div>